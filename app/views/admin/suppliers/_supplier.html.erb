<tr>
  <td><%= supplier.company_name %></td>
  <td><%= supplier.contact_name %></td>
  <td><%= supplier.city %></td>
  <td><%= supplier.homepage %></td>
  <td><%= link_to "", edit_admin_supplier_path(supplier), class: "glyphicon glyphicon-wrench" %></td>
